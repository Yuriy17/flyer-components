{
  "name": "debounced-search",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start:json-server": "node -r dotenv/config ./node_modules/.bin/json-server --watch db.json --port 3000",
    "lint": "eslint src -c eslint.config.js",
    "lint-fix": "eslint src -c eslint.config.js --fix",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings 0",
      "git add"
    ],
    "*.{js, jsx, json, html, css}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "eslint": "^9.5.0",
    "globals": "^15.6.0",
    "husky": "^9.0.11",
    "json-server": "^1.0.0-beta.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "@shoelace-style/shoelace": "^2.15.1",
    "air-datepicker": "^3.5.3",
    "dotenv": "^16.4.5",
    "sass": "^1.77.2",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-html-template": "^1.2.2"
  }
}
