<% if(env.label){ %>
  <label
    <% if(env.id){ %>for="<%=env.id%>" <% } %>
  >
    <%=env.label%>
  </label>
  <% } %>
<input 
  <% if(env.classes){ %>class="<%=env.classes%>" <% } %>
  <% if(env.name){ %>name="<%=env.name%>" <% } %>
  <% if(env.id){ %>id="<%=env.id%>" <% } %>
  <% if(env.placeholder){ %>placeholder="<%=env.placeholder%>" <% } %>
/>
<%- include('templates/layouts/tooltips/fieldTooltip.ejs') %>