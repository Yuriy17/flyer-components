<div class="dynamic-group container-field-row">
  <% if(env.staticContentObject){ %>
    <%- include(env.staticContentObject.dynamicGroupContent.path, {
      env: env.staticContentObject.dynamicGroupContent.params
    }) %>
  <% } else if (env.dynamicContent) { %>
   <%- env.dynamicContent %>
  <% } %>

  <div class="dynamic-group__buttons">
    <div class="dynamic-group__button-block">
      <% if(env.groupIndex < 1){ %>
        <% if(env.staticContentObject){ %>
          <%- include(env.staticContentObject.buttonAdd.path, {
            env: env.staticContentObject.buttonAdd.params
          }) %>
        <% } else if (env.buttonAdd ) { %>
          <%- env.buttonAdd %>
        <% } %>
      <% } %>
    </div>
    <div class="dynamic-group__button-block">
      <% if(env.staticContentObject){ %>
        <%- include(env.staticContentObject.buttonDelete.path, {
          env: env.staticContentObject.buttonDelete.params
        }) %>
      <% } else if (env.buttonDelete) { %>
        <%- env.buttonDelete %>
      <% } %>
    </div>
  </div>
</div>

