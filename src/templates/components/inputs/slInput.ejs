<sl-input 
  <% if(env.classes){ %>class="<%=env.classes%>" <% } %>
  <% if(env.name){ %>name="<%=env.name%>" <% } %>
  <% if(env.id){ %>id="<%=env.id%>" <% } %>
  <% if(env.validate){ %>data-validate="<%=env.validate%>" <% } %>
  <% if(env.label){ %>label="<%=env.label%>" <% } %>
  <% if(env.placeholder){ %>placeholder="<%=env.placeholder%>" <% } %>
  <% if(env.autocomplete){ %>autocomplete="<%=env.autocomplete%>" <% } %>
>
  <% if(env.staticContentPath){ %>
    <%- include(env.staticContentPath) %>
  <% } else{ %>
    <%- env.content %>
  <% } %>
</sl-input>