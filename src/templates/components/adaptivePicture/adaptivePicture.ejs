<picture <% if(!env.isNotLazy){ %>class="lazy-picture"<% } %>>
  <source
    type="image/webp"
    media="(min-width: 576px)"
    srcset="<%= process.env['VITE_STATIC_PATH'] %>/images/desktop/<%= env.fileName %>.webp, <%= process.env['VITE_STATIC_PATH'] %>/images/desktop/<%= env.fileName %>@2x.webp 2x"
  />
  <source
    type="image/<%= env.type %>"
    media="(min-width: 576px)"
    srcset="<%= process.env['VITE_STATIC_PATH'] %>/images/desktop/<%= env.fileName %>.<%= env.type %>, <%= process.env['VITE_STATIC_PATH'] %>/images/desktop/<%= env.fileName %>@2x.<%= env.type %> 2x"
  />
  <source
    type="image/webp"
    srcset="<%= process.env['VITE_STATIC_PATH'] %>/images/mobile/<%= env.fileName %>.webp, <%= process.env['VITE_STATIC_PATH'] %>/images/mobile/<%= env.fileName %>@2x.webp"
  />
  <source
    type="image/<%= env.type %>"
    srcset="<%= process.env['VITE_STATIC_PATH'] %>/images/mobile/<%= env.fileName %>.<%= env.type %>, <%= process.env['VITE_STATIC_PATH'] %>/images/mobile/<%= env.fileName %>@2x.<%= env.type %>"
  />
  <img
    src="<%= process.env['VITE_STATIC_PATH'] %>/images/mobile/<%= env.fileName %>.<%= env.type %>"
    srcset="<%= process.env['VITE_STATIC_PATH'] %>/images/mobile/<%= env.fileName %>@2x.<%= env.type %> 2x"
    <% if(env.imageClass){ %>class="<%=env.imageClass%>"<% } %>
    <% if(env.alt){ %>alt="<%=env.alt%>"<% } %>
    <% if(env.width){ %>width="<%=env.defaultWidth%>"<% } %>
    <% if(env.height){ %>height="<%=env.defaultHeight%>"<% } %>
    <% if(!env.isNotLazy){ %>loading="lazy"<% } %>
    
  />
  <% if(!env.isNotLazy){ %><div class="loading-container"></div><% } %>
</picture>
